<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.assa.mapper.PBoardMapper">
	
	<insert id="addFile">
		insert into		file(
					file_name,
					board_index
		)
		values(
					#{file_name},
					LAST_INSERT_ID()
		)
	</insert>
	
</mapper>
  